{
    "version": 2,
    "customTemplates": [
        {
            "name": "blank",
            "title": "Blank",
            "postTypes": ["page", "post"]
        },
        {
            "name": "no-header",
            "title": "Kein Seitenkopf",
            "postTypes": ["page", "post"]
        },
        {
            "name": "single-event",
            "title": "Event",
            "postTypes": ["event"]
        }
    ],
    "settings": {
        "appearanceTools": true,
        "radius": true,
        "spacing": {
            "margin": true,
            "padding": true
        },
        "custom": {
            "line-height": {
                "body": 1.7,
                "heading": 1.3
            }
        },
        "layout": {
            "contentSize": "1440px",
            "wideSize": "1440px"
        },
        "typography": {
            "fluid": true,
            "letterSpacing": true,
            "fontFamilies": [
                {
                    "fontFamily": "Gochi Hand",
                    "slug": "gochi",
                    "name": "Gochi Hand"
                }
            ]
        }
    },
    "styles": {
        "spacing": {
            "margin": {
                "top": "5rem",
                "right": "auto",
                "bottom": "60px",
                "left": "auto"
            },
            "padding": {
                "left": "1rem",
                "right": "1rem"
            }
        },
        "elements": {
            "link": {
                "typography": {
                    "textDecoration": "none"
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "test",
            "title": "Footer",
            "area": "footer"
        }
    ]
}
