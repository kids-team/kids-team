{
    "version": 2,
    "customTemplates": [
        {
            "name": "blank",
            "title": "Blank",
            "postTypes": ["page", "post"]
        },
        {
            "name": "no-header",
            "title": "Kein Seitenkopf",
            "postTypes": ["page", "post"]
        },
        {
            "name": "single-event",
            "title": "Event",
            "postTypes": ["event"]
        }
    ],
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": true,
        "radius": true,
        "dimensions": {
            "minHeight": true
        },
        "spacing": {
            "margin": true,
            "padding": true,
            "units": ["rem"],
            "blockGap": true
        },
        "custom": {
            "line-height": {
                "body": 1.7,
                "heading": 1.3
            }
        },
        "layout": {
            "contentSize": "1440px",
            "wideSize": "1440px"
        },
        "typography": {
            "fluid": true,
            "letterSpacing": true,
            "fontFamilies": [
                {
                    "fontFamily": "Fuzzy Bubbles",
                    "slug": "fuzzy-bubbles",
                    "name": "Fuzzy Bubbles"
                }
            ]
        },
        "blocks": {
            "core/heading": {
                "supports": {
                    "radius": true,
                    "typography": {
                        "fontSize": true,
                        "lineHeight": true,
                        "letterSpacing": true,
                        "fontFamily": true,
                        "fontWeight": true,
                        "textTransform": true,
                        "textDecoration": true,
                        "fontStyle": true,
                        "color": true
                    }
                }
            },
            "core/group": {
                "spacing": {
                    "padding": "4"
                }
            }
        }
    },
    "styles": {
        "spacing": {
            "margin": {
                "top": "5rem",
                "right": "auto",
                "bottom": "60px",
                "left": "auto"
            },
            "padding": {
                "left": "1.5rem",
                "right": "1.5rem"
            }
        },
        "elements": {
            "link": {
                "typography": {
                    "textDecoration": "none"
                }
            }
        },
        "blocks": {
            "core/cover": {
                "supports": {
                    "radius": true
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "test",
            "title": "Footer",
            "area": "footer"
        }
    ]
}
